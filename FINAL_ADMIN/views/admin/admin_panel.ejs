<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.10.1/js/mdb.min.js"></script>
<script>
    $(document).ready(()=>{
        let status=false;
        $('#btn1').on('click',(e)=>{
            location.href = 'https://localhost:8000/rover_manual';
        });
             $('#btn3').on('click',(e)=>{
                $('#coords').fadeToggle('slow');
                
            });
    })

</script>
