            <div id='coords'class="mt-4" style="display: none;">
                <table id="dtMaterialDesignExample" class="table table-striped" cellspacing="0" width="40%">
                    <thead>
                      <tr>
                        <th></th>
                        <th id='align'class="th-md">
                            Co-ordinates
                        </th>
                        <th></th>
                      </tr>
                      <tr>
                        <th>Name</th>
                        <th>Latitude</th>
                        <th>Longitude</th>
                      </tr>
                    </thead>
                    <tbody>
                    </tbody>
                  </table>
            </div>
        </div>
    </div>
    
  
   <!-- JQuery -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<!-- Bootstrap tooltips -->
